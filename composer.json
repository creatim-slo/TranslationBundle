{
  "name": "creatim-slo/translation-bundle",
  "description": "Symfony Tool to help Import/Export (missing) translations",
  "keywords": ["symfony", "translation"],
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Michel NAUD",
      "role": "Author"
    },
    {
      "name": "Andrius Petrauskis",
      "role": "Author"
    }
  ],
  "require": {
    "php": "^8.1",
    "symfony/yaml": "^5.4|^6.0",
    "symfony/finder": "^5.4|^6.0",
    "symfony/filesystem": "^5.4|^6.0",
    "symfony/console": "^5.4|^6.0",
    "league/csv": "^9.8"
  },
  "require-dev": {
    "symfony/http-kernel": "^5.4|^6.0",
    "symfony/phpunit-bridge": "^5.4|^6.0",
    "symfony/dependency-injection": "^5.4|^6.0"
  },
  "autoload": {
    "psr-4": {
      "CavernBay\\TranslationBundle\\": "src"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "CavernBay\\TranslationBundle\\Tests\\": "tests"
    }
  }
}
